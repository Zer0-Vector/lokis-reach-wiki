<!DOCTYPE svg>
<svg
  xmlns="http://www.w3.org/2000/svg"
 {{> attributes svg }}
>
  {{#each rows}}
  <svg {{> attributes this.subsvg }}>
    <rect id="row-bg-{{@index}}" {{> attributes block }} />
    {{#each swatches}}
    {{#with this}}
    <rect id="swatch-{{@index}}" {{>  attributes }} />
    {{/with}}
    {{/each}}
    </svg>
  {{/each}}
</svg>
