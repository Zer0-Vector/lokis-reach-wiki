{
  "name": "lokis-reach-wiki",
  "version": "1.0.0",
  "private": true,
  "description": "A wiki for a Stars Without Number galaxy sector and homebrew rules.",
  "config": {
    "sdconfig": "./styled.config.js"
  },
  "scripts": {
    "compile": "env-cmd --silent -x style-dictionary build --config $npm_package_config_sdconfig",
    "clean": "env-cmd --silent -x style-dictionary clean --config $npm_package_config_sdconfig",
    "build": "npm run clean && npm run compile",
    "deploy": "env-cmd gulp deploy-styles",
    "env": "env-cmd env"
  },
