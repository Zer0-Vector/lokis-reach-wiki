    "@types/eslint-config-prettier": "^6.11.3",
    "env-cmd": "^10.1.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "esm": "^3.2.25",
    "fancy-log": "^2.0.0",
    "globals": "^15.11.0",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "markdownlint": "^0.36.1",
    "minimist": "^1.2.8",
    "prettier": "3.3.3",
    "style-dictionary": "^4.1.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
